<div class="page row">
    <div class="column">
        <div class="page-content">
            <div class="card">
                <div class="card-header">
                    <figure class="logo-wrapper">
                        <a href="https://sensenet.com" target="_blank">
                            <img class="logo" src="/images/sensenet-logo.png" alt="sensenet logo" />
                            <figcaption class="logo-caption">sensenet</figcaption>
                        </a>
                    </figure>
                </div>

                <div class="card-body">
                    <h1 class="card-title">Unsuccessful registration</h1>


                    <p class="text-center">
                        to <a href="@Model.SnRepositoryUrl" target="_blank"><strong>@Model.SnRepositoryUrl</strong></a>
                    </p>

                    <p class="alert">
                        <strong>Your registration was not successful, please try again later.</strong>
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>